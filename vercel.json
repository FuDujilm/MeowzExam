{
  "buildCommand": "prisma generate && next build",
  "devCommand": "next dev --turbopack -p 3001",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["hkg1"],
  "env": {
    "DATABASE_URL": "@database_url",
    "AUTH_SECRET": "@auth_secret",
    "OAUTH_CLIENT_ID": "@oauth_client_id",
    "OAUTH_CLIENT_SECRET": "@oauth_client_secret",
    "OAUTH_BASE_URL": "@oauth_base_url",
    "NEXT_PUBLIC_OAUTH_BASE_URL": "@oauth_base_url",
    "OPENAI_API_KEY": "@openai_api_key",
    "OPENAI_BASE_URL": "@openai_base_url",
    "JWT_SECRET": "@jwt_secret",
    "WECHAT_MINIAPP_APPID": "@wechat_miniapp_appid",
    "WECHAT_MINIAPP_SECRET": "@wechat_miniapp_secret",
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_pass",
    "SMTP_FROM": "@smtp_from",
    "FEEDBACK_RECEIVER_EMAIL": "@feedback_receiver_email",
    "CF_R2_ACCOUNT_ID": "@cf_r2_account_id",
    "CF_R2_ACCESS_KEY_ID": "@cf_r2_access_key_id",
    "CF_R2_SECRET_ACCESS_KEY": "@cf_r2_secret_access_key",
    "CF_R2_BUCKET_NAME": "@cf_r2_bucket_name",
    "CF_R2_PUBLIC_BASE_URL": "@cf_r2_public_base_url",
    "NEXT_PUBLIC_TURNSTILE_SITE_KEY": "@turnstile_site_key",
    "TURNSTILE_SECRET_KEY": "@turnstile_secret_key",
    "ADMIN_EMAILS": "@admin_emails"
  }
}
